<Window x:Class="militreg_lite.UI.AddToDict"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:militreg_lite.UI"
        mc:Ignorable="d"
        Title="AddToDict" Height="150" Width="200" SizeToContent="WidthAndHeight">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="tbName" Width="200" Height="20" ></TextBox>
        <DockPanel Grid.Row="1" Height="30" Margin="0,10,0,0">
            <Button Content="Відмінити" Click="ButtonCancel_Click" Width="100"></Button>
            <Button Content="Ок" Click="ButtonOk_Click"></Button>
        </DockPanel>
    </Grid>
</Window>
